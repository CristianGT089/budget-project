---
interface BudgeItem {
    type: string;
    subtype: string;
    category: string;
    amount: number;
    date: Date;
}


const {type, subtype, category, amount, date} = Astro.props;
---

<div class="flex flex-col-reverse gap-5">
    <p>{type}</p>
    <p>{subtype}</p>
    <p>{category}</p>
    <p>{amount}</p>
    <time datetime="">{date}</time>
</div>